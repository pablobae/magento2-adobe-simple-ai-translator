    "type": "magento2-module",
    "version": "1.0.0",
    "require": {
        "php": ">=8.1",
        "magento/framework": "^103.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": [
        "GPL-3.0-or-later"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Pablobae\\SimpleAiTranslator\\": ""
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Pablo César Baenas Castelló",
